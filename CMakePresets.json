{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 28,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "base",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "${sourceParentDir}/halide-build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_INSTALL_PREFIX": "${sourceParentDir}/halide-install",
        "CMAKE_PREFIX_PATH": "$env{VIRTUAL_ENV}",
        "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
        "Halide_BUILD_HEXAGON_REMOTE_RUNTIME": "OFF",
        "Halide_LLVM_ROOT": "$env{HALIDE_LLVM_ROOT}",
        "Halide_TARGET": "${presetName}",
        "WITH_PYTHON_BINDINGS": "ON",
        "WITH_SERIALIZATION_JIT_ROUNDTRIP_TESTING": "OFF",
        "WITH_TEST_FUZZ": "OFF"
      }
    },
    {
      "name": "wasm-wabt",
      "hidden": true,
      "cacheVariables": {
        "Halide_WASM_BACKEND": "wabt",
        "V8_INCLUDE_PATH": "",
        "V8_LIB_PATH": "",
        "WITH_PYTHON_BINDINGS": "OFF"
      }
    },
    {
      "name": "wasm-v8",
      "hidden": true,
      "cacheVariables": {
        "Halide_WASM_BACKEND": "V8",
        "V8_INCLUDE_PATH": "/home/halidenightly/v8/v8/include",
        "V8_LIB_PATH": "/home/halidenightly/v8/v8/out/x64.release.static/obj/libv8_monolith.a",
        "WITH_PYTHON_BINDINGS": "OFF"
      }
    },
    {
      "name": "host",
      "inherits": "base"
    },
    {
      "name": "host-cuda",
      "inherits": "base"
    },
    {
      "name": "host-cuda-opencl",
      "inherits": "base"
    },
    {
      "name": "host-hvx",
      "inherits": "base",
      "cacheVariables": {
        "Halide_BUILD_HEXAGON_REMOTE_RUNTIME": "ON"
      }
    },
    {
      "name": "host-metal",
      "inherits": "base"
    },
    {
      "name": "host-opencl",
      "inherits": "base"
    },
    {
      "name": "host-vulkan-vk_int8-vk_int16-vk_int64-vk_float16-vk_float64-vk_v13",
      "inherits": "base"
    },
    {
      "name": "host-webgpu",
      "inherits": "base"
    },
    {
      "name": "host.serialization_jit_testing",
      "inherits": "base",
      "cacheVariables": {
        "Halide_TARGET": "host",
        "WITH_SERIALIZATION_JIT_ROUNDTRIP_TESTING": "ON"
      }
    },
    {
      "name": "wasm-32-wasmrt-wasm_mvponly.wabt",
      "inherits": [
        "base",
        "wasm-wabt"
      ],
      "cacheVariables": {
        "Halide_TARGET": "wasm-32-wasmrt-wasm_mvponly"
      }
    },
    {
      "name": "wasm-32-wasmrt-wasm_simd128-wasm_threads.wabt",
      "inherits": [
        "base",
        "wasm-wabt"
      ],
      "cacheVariables": {
        "Halide_TARGET": "wasm-32-wasmrt-wasm_simd128-wasm_threads"
      }
    },
    {
      "name": "wasm-32-wasmrt-wasm_simd128.V8",
      "inherits": [
        "base",
        "wasm-v8"
      ],
      "cacheVariables": {
        "Halide_TARGET": "wasm-32-wasmrt-wasm_simd128"
      }
    },
    {
      "name": "wasm-32-wasmrt-wasm_simd128.wabt",
      "inherits": [
        "base",
        "wasm-wabt"
      ],
      "cacheVariables": {
        "Halide_TARGET": "wasm-32-wasmrt-wasm_simd128"
      }
    },
    {
      "name": "wasm-32-wasmrt-webgpu.wabt",
      "inherits": [
        "base",
        "wasm-wabt"
      ],
      "cacheVariables": {
        "Halide_TARGET": "wasm-32-wasmrt-webgpu"
      }
    },
    {
      "name": "x86-32-linux",
      "inherits": "base"
    },
    {
      "name": "x86-32-windows",
      "inherits": "base"
    },
    {
      "name": "x86-64-linux",
      "inherits": "base"
    },
    {
      "name": "x86-64-linux-sse41",
      "inherits": "base"
    },
    {
      "name": "x86-64-osx",
      "inherits": "base"
    },
    {
      "name": "x86-64-osx-sse41",
      "inherits": "base"
    },
    {
      "name": "x86-64-windows",
      "inherits": "base"
    },
    {
      "name": "x86-64-windows-sse41",
      "inherits": "base"
    }
  ]
}
